#include <iostream>
#include <math.h>
#include <time.h>
#include <stdlib.h>
#include <iomanip>
using namespace std;

int main()
{
srand(time(0));
const int n = 6, m = 5;
int i,j,arr[n][m],k=0,kol=0;
cout<<"Massiv= "<<endl;
for (i=0;i<n;i++)
{
for (j=0;j<m;j++)
{
arr[i][j]=rand()%20-10;
cout<<setw(5)<<arr[i][j];
}
cout << endl;
}
for (j=0;j<n;j++)
{
for (i=0;i<m;i++)
{
if (arr[j][i]==0)
{k++;
break;
}
}
}
cout << k;

}
